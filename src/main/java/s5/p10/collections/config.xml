<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean name="collectionBean" class="s5.p10.collections.CollectionImportXml" >
        <property name="properties">
            <props>
                <prop key="name"> prop1</prop>
                <prop key="name2"> prop2</prop>
                <prop key="name3"> prop3</prop>
                <prop key="name4"> prop4</prop>
                <prop key="name5"> prop5</prop>
            </props>
        </property>
        <property name="lists">
            <list>
                <value>list 1</value>
                <value>list 2</value>
            </list>
        </property>
        <property name="maps">
            <map>
                <entry key="name1" value="map1" />
                <entry key="name2" value="map2" />
                <entry key="name3" value="map3" />
                <entry key="name4" value="map4" />
            </map>
        </property>
        <property name="sets">
            <set>
                <value>set1</value>
                <value>set2</value>
                <value>set3</value>
                <value>set4</value>
            </set>
        </property>
    </bean>
</beans>